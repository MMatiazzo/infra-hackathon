name: Terraform Create Database Autenticacao Workflow

on:
  push:
    branches:
      - master
    paths:
      - 'db/autenticacao/**'
  pull_request:
    branches:
      - master
    paths:
      - 'db/autenticacao/**'
  workflow_dispatch:

jobs:
  print_success:
    runs-on: ubuntu-latest
    steps:
      - name: Checkout code
        uses: actions/checkout@v2
      - name: Print Success Message
        run: echo "Successful execution!"